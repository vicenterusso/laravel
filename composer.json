{
    "name": "vicenterusso/laravel-model-generator",
    "description": "Model code generation for Laravel Framework.",
    "keywords": ["vicenterusso", "laravel", "model", "generator", "relations", "relationships"],
    "support": {
        "issues": "https://github.com/vicenterusso/laravel-model-generator/issues",
        "source": "https://github.com/vicenterusso/laravel-model-generator"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vicente Russo",
            "email": "vicente.russo@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "doctrine/dbal": ">=2.5",
        "doctrine/annotations" : ">=1.10",
        "illuminate/support": ">=5.1",
        "illuminate/database": ">=5.1",
        "illuminate/contracts": ">=5.1",
        "illuminate/filesystem": ">=5.1",
        "illuminate/console": ">=5.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "VRusso\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "VRusso\\Coders\\CodersServiceProvider"
            ]
        }
    }
}
